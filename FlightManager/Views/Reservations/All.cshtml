@model IEnumerable<ReservationBasicViewModel>

@{
    this.ViewData["Title"] = "All Passengers";
}

<h1 class="text-center">Passengers</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var reservation in this.Model)
        {
            <tr>
                <td>@reservation.Id</td>
                <td>@reservation.Email</td>
            </tr>
        }
    </tbody>
</table>
